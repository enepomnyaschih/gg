@import "../../imports"

.gg-about-page
	&-logo
		padding 0 20px

	&-container
		position relative

		ol, ul
			line-height 1.3
			text-align justify

	@media screen and (min-width: 960px)
		&-logo
			background scroll transparent url(/images/logo.svg) no-repeat 20px 50px
			background-size 100px 100px
			margin 0 auto
			width 880px

		&-container
			margin 0 auto
			width 640px
